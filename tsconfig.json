{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["esnext", "dom"],
"types": ["@dcloudio/types","@types/wechat-miniprogram","@vue/runtime-core"]

},
"vueCompilerOptions": {

"nativeTags": ["block","component","template","slot"]

}, 
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
